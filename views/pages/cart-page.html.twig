{#
 # Cart page
 #
 # @package WordPress
 # @subpackage Rider404
 #}
{% extends 'index.html.twig' %}


{% block hero %}
	 <div class="Hero" data-scroll-section>
        <div class="row">
            <div class="col-10 offset-1">
				<h1 class="text-align-center m-0">
					{{-
						include(
							'partials/split-text.html.twig',
							{
								title: post.title,
								id: uniqid(),
								delay: 0.5,
							},
							with_context = false
						)
					-}}
				</h1>
			</div>
		</div>
	</div>
{% endblock %}

{% block content %}
	{{- post.content -}}
{% endblock  %}
